<!--
    Name: AnimatedHeader
    Author: Josh Miccolo
    Description: Header block for the Trial Of The Titans Hero Hub. 
    Changes with the current leader of Teams.
-->

<aura:component>
    <aura:attribute name="leadTeams" type="Map"/>
